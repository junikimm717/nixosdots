{
  "diagnostic-languageserver.filetypes": {
    "sh": ["shellcheck", "-x"]
  },
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "json",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "svelte",
    "php"
  ],
  "html.filetypes": [
    "html",
    "handlebars",
    "htmldjango",
    "vue",
    "blade",
    "svelte"
  ],
  "languageserver": {
    "nix": {
      "command": "rnix-lsp",
      "filetypes": ["nix"]
    }
  },
  "coc.source.vimtex.filetypes": ["tex", "plaintex", "latex"]
}
